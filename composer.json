{
    "name": "nuonic/searchhub-integration",
    "description": "SearchHub Integration",
    "type": "shopware-platform-plugin",
    "version": "1.0.1",
    "license": "proprietary",
    "authors": [
        {
            "name": "nuonic Digital GmbH & Co. KG",
            "homepage": "https://nuonic.de"
        }
    ],
    "extra": {
        "copyright": "nuonic Digital GmbH & Co. KG",
        "author": "nuonic Digital GmbH & Co. KG",
        "shopware-plugin-class": "NuonicSearchHub\\NuonicSearchHub",
        "label": {
            "de-DE": "SearchHub Integration",
            "en-GB": "SearchHub Integration"
        },
        "description": {
            "de-DE": "searchHub ist ein KI-gestütztes Add-on, das jede E-Commerce-Suchplattform optimiert. Es analysiert und verbessert Suchanfragen automatisch für präzisere, konversionsstarke Ergebnisse.",
            "en-GB": "searchHub is an AI-powered add-on that optimizes any e-commerce search platform. It automatically analyzes and improves search queries for more precise, conversion-driving results."
        },
        "manufacturerLink": {
            "de-DE": "https://nuonic.de",
            "en-GB": "https://nuonic.de"
        },
        "supportLink": {
            "de-DE": "https://nuonic.de",
            "en-GB": "https://nuonic.de"
        }
    },
    "require": {
        "shopware/core": ">=6.6.0 <6.7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.2",
        "friendsofphp/php-cs-fixer": "^3.66",
        "shopwarelabs/phpstan-shopware": "^0.1.8"
    },
    "autoload": {
        "psr-4": {
            "NuonicSearchHub\\": "src/"
        }
    },
    "scripts": {
        "phpstan": "phpstan analyse --memory-limit=2G",
        "style-check": "php-cs-fixer check",
        "style-fix": "php-cs-fixer fix"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
